<section class="talks">
  <% settings.talks.each_with_index do |talk, index| %>
    <article class="talk">
      <h2 class="talk-title"><a href="<%= talk.url %>" tabindex="<%= index + 1 %>"><%= talk.title %></a></h2>
      <p class="talk-description"><%= markdown talk.content %></p>
    </article>
  <% end %>
</section>
